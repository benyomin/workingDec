	Issn = {0049089X},
	Journal = {Social Science Research},
	Keywords = {Earnings gaps,Earnings structure,Ethnic gaps,Gender gaps,Inequality,Israel,National gaps},
	Number = {2},
	Pages = {654--672},
	Title = {{Gender, ethnic, and national earnings gaps in Israel: The role of rising inequality}},
