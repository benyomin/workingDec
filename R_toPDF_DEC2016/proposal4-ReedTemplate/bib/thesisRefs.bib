	Db = {PMC},
	Isbn = {0033-3549; 1468-2877},
	J1 = {Public Health Rep},
	Journal = {Public Health Reports},
	Month = {Nov-Dec},
	Number = {6},
	Pages = {753--763},
	Publisher = {Association of Schools of Public Health},
	Title = {Potential Implications of Missing Income Data in Population-Based Surveys: An Example from a Postpartum Survey in California},
	Ty = {JOUR},
	U1 = {18051668{$[$}pmid{$]$}; 8{\_}Kim{$[$}PII{$]$}; 18051668{$[$}pmid{$]$}},
